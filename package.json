{
  "name": "express-typescript-seed",
  "version": "1.0.0",
  "description": "express seed",
  "scripts": {
    "test:integration": "gulp build:test & cross-env NODE_ENV=test gulp test:integration",
    "test:unit": "gulp build:test & cross-env NODE_ENV=test gulp test:unit",
    "coverage:integration": "nyc --reporter=html --report-dir=report/integration-test npm run test:integration",
    "coverage:unit": "nyc --reporter=html --report-dir=report/unit-test npm run test:unit",
    "build": "gulp build:src",
    "watch": "gulp watch",
    "release": "gulp release:gzip",
    "lint:integration": "tslint -c tslint.json 'test/integration/**/*.ts'",
    "lint:unit": "tslint -c tslint.json 'test/unit/**/*.ts'",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint": "npm run lint:src & npm run lint:unit & npm run lint:integration",
    "start": "cross-env NODE_ENV=development nodemon --delay 5 build/dist/index",
    "test": "npm run lint & gulp build:test & cross-env NODE_ENV=test gulp test:unit & cross-env NODE_ENV=test gulp test:integration",
    "clean": "gulp clean:all",
    "cluster": "cross-env NODE_ENV=development node build/dist/cluster"
  },
  "author": "Jonhatan Haisi",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.1",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^5.2.6",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.36",
    "@types/supertest": "^2.0.7",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-mocha": "^6.0.0",
    "gulp-tar": "^2.1.0",
    "gulp-typescript": "^5.0.0",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "supertest": "^3.4.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "cluster": "^0.7.7",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "helmet": "^3.15.1",
    "http": "0.0.0",
    "http-status": "^1.3.1",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "sequelize": "^4.42.0",
    "winston": "^3.2.1"
  }
}
